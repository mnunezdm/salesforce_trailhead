<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignments>
        <name>Map_Contact_ID_to_Campaign_Member</name>
        <label>Map Contact ID to Campaign Member</label>
        <locationX>479</locationX>
        <locationY>267</locationY>
        <assignmentItems>
            <assignToReference>CampaignMemberRecord.CampaignId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>CampaignIDFromPB</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>CampaignMemberRecord.ContactId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>ContactRecord.Id</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Populate_the_Campaign_Member_Collection</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Populate_the_Campaign_Member_Collection</name>
        <label>Populate the Campaign Member Collection</label>
        <locationX>666</locationX>
        <locationY>277</locationY>
        <assignmentItems>
            <assignToReference>TACampaignMembers</assignToReference>
            <operator>Add</operator>
            <value>
                <elementReference>CampaignMemberRecord</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Loop_through_contacts</targetReference>
        </connector>
    </assignments>
    <description>Add local potential Teaching Assistants as Campaign Members on the Class Session campaign</description>
    <interviewLabel>Add Chapter TAs to Class Session {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Add Chapter TAs to Class Session</label>
    <loops>
        <name>Loop_through_contacts</name>
        <label>Loop through contacts</label>
        <locationX>475</locationX>
        <locationY>123</locationY>
        <assignNextValueToReference>ContactRecord</assignNextValueToReference>
        <collectionReference>ChapterTAContacts</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Map_Contact_ID_to_Campaign_Member</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Create_Campaign_Members_for_TAs</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processType>AutoLaunchedFlow</processType>
    <recordCreates>
        <name>Create_Campaign_Members_for_TAs</name>
        <label>Create Campaign Members for TAs</label>
        <locationX>676</locationX>
        <locationY>123</locationY>
        <inputReference>TACampaignMembers</inputReference>
    </recordCreates>
    <recordLookups>
        <name>Find_Chapter_TAs</name>
        <label>Find Chapter TAs</label>
        <locationX>246</locationX>
        <locationY>137</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Loop_through_contacts</targetReference>
        </connector>
        <filters>
            <field>AccountId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>AccountIDFromPB</elementReference>
            </value>
        </filters>
        <object>Contact</object>
        <outputReference>ChapterTAContacts</outputReference>
        <queriedFields>Id</queriedFields>
    </recordLookups>
    <startElementReference>Find_Chapter_TAs</startElementReference>
    <variables>
        <name>AccountIDFromPB</name>
        <dataType>String</dataType>
